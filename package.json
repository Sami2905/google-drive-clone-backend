{
  "name": "file-storage-backend",
  "version": "1.0.0",
  "description": "Backend for Google Drive clone with file storage and sharing capabilities",
  "main": "dist/supabase-server.js",
  "scripts": {
    "start": "node dist/supabase-server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/supabase-server.ts",
    "dev-supabase": "ts-node-dev --respawn --transpile-only src/supabase-server.ts",
    "build": "tsc --project tsconfig.backend.json",
    "build:frontend": "cd drive-frontend && npm run build",
    "build:all": "npm run build && npm run build:frontend",
    "build:production": "npm run clean && tsc --project tsconfig.production.json",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false --verbose",
    "test:coverage": "jest --coverage",
    "test:supabase": "jest --config jest.config.supabase.js",
    "test:build": "npm run build && node dist/supabase-server.js --test",
    "test:integration": "jest --config jest.config.supabase.js --testPathPattern=integration",
    "test:unit": "jest --config jest.config.supabase.js --testPathPattern=unit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "setup-firebase": "node scripts/setup-firebase.js",
    "docker:build": "docker build -f Dockerfile.production -t google-drive-clone:latest .",
    "docker:run": "docker run -p 5000:5000 --env-file .env.production google-drive-clone:latest",
    "docker:compose": "docker-compose -f docker-compose.production.yml up -d",
    "docker:compose:down": "docker-compose -f docker-compose.production.yml down",
    "deploy:production": "bash scripts/deploy-production.sh deploy",
    "deploy:rollback": "bash scripts/deploy-production.sh rollback",
    "deploy:health": "bash scripts/deploy-production.sh health-check",
    "deploy:backup": "bash scripts/deploy-production.sh backup",
    "pm2:start": "pm2 start ecosystem.config.production.js --env production",
    "pm2:stop": "pm2 stop google-drive-clone-api",
    "pm2:restart": "pm2 restart google-drive-clone-api",
    "pm2:reload": "pm2 reload google-drive-clone-api",
    "pm2:logs": "pm2 logs google-drive-clone-api",
    "pm2:monitor": "pm2 monit",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:check": "npm audit && snyk test",
    "performance:test": "artillery run tests/performance/load-test.yml",
    "performance:profile": "node --prof dist/supabase-server.js",
    "backup:database": "node scripts/backup-database.js",
    "backup:files": "node scripts/backup-files.js",
    "monitoring:setup": "node scripts/setup-monitoring.js",
    "health:check": "curl -f http://localhost:5000/health || exit 1"
  },
  "keywords": [
    "file-storage",
    "google-drive-clone",
    "typescript",
    "express",
    "supabase",
    "production-ready"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
